@model BrnShop.PayPlugin.WeChat.ConfigModel
           
<script type="text/javascript" src="/administration/scripts/jquery.validate.js"></script>
<script type="text/javascript" src="/administration/scripts/jquery.validate.unobtrusive.js"></script>
<h1 class="rightH1">
插件管理 &gt;&gt; 插件配置
<div class="right">
<div class="clear"></div>
</div>
<div class="clear"></div>
</h1>
@using (Html.BeginForm("config", "adminwechat"))
{
<div class="addTable">
<table width="100%">
  <tr>
    <td width="120px" align="right">WPMchId：</td>
    <td>
    @Html.TextBoxFor(x => x.WPMchId, new { @class = "input", size = "20" })
    @Html.ValidationMessageFor(x => x.WPMchId)
    </td>
    </tr>
      <tr>
    <td align="right">WPAppId：</td>
    <td>
    @Html.TextBoxFor(x => x.WPAppId, new { @class = "input", size = "20" })
    @Html.ValidationMessageFor(x => x.WPAppId)
    </td>
    </tr>
      <tr>
    <td align="right">WPAppSecret：</td>
    <td>
    @Html.TextBoxFor(x => x.WPAppSecret, new { @class = "input", size = "20" })
    @Html.ValidationMessageFor(x => x.WPAppSecret)
    </td>
    </tr>
          <tr>
    <td align="right">WPAppKey：</td>
    <td>
    @Html.TextBoxFor(x => x.WPAppKey, new { @class = "input", size = "20" })
    @Html.ValidationMessageFor(x => x.WPAppKey)
    </td>
    </tr>
      <tr>
    <td align="right">OpenMchId：</td>
    <td>
    @Html.TextBoxFor(x => x.OpenMchId, new { @class = "input", size = "20" })
    @Html.ValidationMessageFor(x => x.OpenMchId)
    </td>
    </tr>
      <tr>
    <td align="right">OpenAppId：</td>
    <td>
    @Html.TextBoxFor(x => x.OpenAppId, new { @class = "input", size = "20" })
    @Html.ValidationMessageFor(x => x.OpenAppId)
    </td>
    </tr>
      <tr>
    <td align="right">OpenAppSecret：</td>
    <td>
    @Html.TextBoxFor(x => x.OpenAppSecret, new { @class = "input", size = "20" })
    @Html.ValidationMessageFor(x => x.OpenAppSecret)
    </td>
    </tr>
          <tr>
    <td align="right">OpenAppKey：</td>
    <td>
    @Html.TextBoxFor(x => x.OpenAppKey, new { @class = "input", size = "20" })
    @Html.ValidationMessageFor(x => x.OpenAppKey)
    </td>
    </tr>
  <tr>
    <td align="right">&nbsp;</td>
    <td>
    <a href="" class="addBt" id="sumbitBut" ><img src="/administration/images/submit.jpg" /></a>
            <script type="text/javascript">
                $(function () {
                    $("#sumbitBut").click(function () {
                        $("form:first").submit();
                        return false;
                    })
                });
        </script>
    </td>
    </tr>
  <tr>
    <td align="right">&nbsp;</td>
    <td>&nbsp;</td>
    </tr>
</table>
</div>
}