@model BrnMall.Web.Mobile.Models.CouponTypeListModel
@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>优惠劵列表-@{@WorkContext.MallConfig.SiteTitle}触屏版</title>
    @Html.Raw(WorkContext.MallConfig.SEOKeyword)
    @Html.Raw(WorkContext.MallConfig.SEODescription)
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link href="@{@WorkContext.CSSCDN}/mobile/css/base.css" rel="stylesheet" type="text/css">
    <link href="@{@WorkContext.CSSCDN}/mobile/css/store.css" rel="stylesheet" type="text/css">
    <script src="@{@WorkContext.ScriptCDN}/mobile/scripts/jquery.js" type="text/javascript"></script>
    <script src="@{@WorkContext.ScriptCDN}/mobile/scripts/common.js" type="text/javascript"></script>
    <script type="text/javascript">
        uid=@WorkContext.Uid;
    </script>
</head>

<body>
<div class="viewport">

@Html.Partial("_header")
<script type="text/javascript">
    $("#headerTitle").html("优惠劵列表");
</script>


<div class="yhjBox yhjBox1">

@foreach (CouponTypeInfo info in Model.CouponTypeList)
{
<a href="javascript:getCoupon(@info.CouponTypeId);" class="yhj">
<div class="line"></div>
<p><span class="other">@info.Money</span><span>@info.Name</span><span>@info.SendStartTime.ToShortDateString() - @info.UseEndTime.ToShortDateString()</span></p>
<span class="useBt">领取</span>
</a>
}

</div>


</div>
</body>
</html>
